  title Land certificate 
h4.center.mt-3.bold LAND CERTIFICATES
  .center 
    a.btn.btn-success( href="/api/admin/addland") Certify a land

.table-wrapper
  table.fl-table
    thead
      tr
        th TITRE ID
        th FULL NAME
        th CIN
        th ADDRESS
        th EMAIL
        th INDICE
        th LAND CONSERVER
        th SPECIAL INDICE
        th LINK
    tbody
      each item in list
        tr
          td=item["Key"]
          td=item["Record"]["fullName"]
          td=item["Record"]["cin"]
          td=item["Record"]["address"]
          td=item["Record"]["email"]
          td=item["Record"]["indice"]
          td=item["Record"]["city_fonc"]
          td=item["Record"]["special_indice"]
          td
            a.btn-custom(href="/api/query/" +item["Key"]) view details

